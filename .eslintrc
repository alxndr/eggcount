{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "comma-dangle": 1,
    "max-len": [1, 128],
    "no-unused-vars": [2, {"argsIgnorePattern": "^_"}],
    "no-cond-assign": 1,
    "no-console": 1
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "globals": {
    "global": false,
    "require": false
  },
  "extends": "eslint:recommended"
}
